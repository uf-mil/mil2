<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>subjugator_keyboard_control</name>
  <version>0.0.1</version>
  <description>A teleoperation node that reads keyboard input and publishes wrench messages for subjugator.</description>
  <maintainer email="rickyzhang@ufl.edu">Ricky Zhang</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <build_depend>libsdl2-dev</build_depend>
  <exec_depend>libsdl2-dev</exec_depend>

  <depend>geometry_msgs</depend>
  <depend>rclcpp</depend>
  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
