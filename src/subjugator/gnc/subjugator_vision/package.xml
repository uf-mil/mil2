<?xml version="1.0"?>
<package format="3">
  <name>subjugator_vision</name>
  <version>0.0.0</version>
  <description>Vision utilities for the Subjugator simulator.</description>

  <maintainer email="c.chavez@ufl.edu">Carlos Chavez</maintainer>
  <license>TODO: License declaration</license>

  <!-- Build tool -->
  <buildtool_depend>ament_cmake</buildtool_depend>

  <!-- Runtime (exec) dependencies -->
  <exec_depend>rclpy</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>vision_msgs</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>mil_msgs</exec_depend>
  <exec_depend>cv_bridge</exec_depend>

  <!-- Pure-Python third-party libs (pip) -->
  <exec_depend>numpy</exec_depend>
  <exec_depend>opencv-python</exec_depend>
  <exec_depend>ultralytics</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>